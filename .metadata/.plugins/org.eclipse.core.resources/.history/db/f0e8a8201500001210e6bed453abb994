	function afficheInfoBulle(ancre, texte) {
		if (texte == '') return;
		
		$("body").append('<span class="infoBulle"></span>')
		var bulle = $(".infoBulle");
		bulle.append(texte);
		var posTop = ancre.offset().top - ancre.height();
		var posLeft = ancre.offset().left - ancre.width()/2 - bulle.width()/2;
		bulle.css({
			
		})
	}